#include <stdio.h>

void findDuplicates(int arr[], int size) {
    printf("Tekrar Eden Elemanlar: ");
    
    for (int i = 0; i < size; i++) {
        // Elemanın negatif hale getirilmesi
        int index = abs(arr[i]);
        
        // Eğer elemanın indeksi negatifse, tekrar eden bir elemandır
        if (arr[index] >= 0)
            arr[index] = -arr[index];
        else
            printf("%d ", index);
    }
    
    printf("\n");
}

int main() {
    int arr[] = {1, 2, 3, 4, 4, 2, 7, 8, 8, 9};
    int size = sizeof(arr) / sizeof(arr[0]);
    
    findDuplicates(arr, size);
    
    return 0;
}
